{
  "name": "personal-life-manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "start:static": "NODE_ENV=production bun src/static-server.tsx",
    "build": "bun run build.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "bun db/migrate.ts",
    "db:seed": "bun db/seed.ts",
    "test": "bun test --preload ./tests/setup.ts",
    "test:watch": "bun test --watch --preload ./tests/setup.ts",
    "test:coverage": "bun test --coverage --preload ./tests/setup.ts"
  },
  "dependencies": {
    "@nivo/bar": "^0.99.0",
    "@nivo/calendar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/scatterplot": "^0.99.0",
    "@noble/ciphers": "^2.0.1",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/themes": "^3.2.1",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.4",
    "bun-plugin-tailwind": "^0.1.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "ioredis": "^5.7.0",
    "jose": "^6.1.0",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.3",
    "react": "^19",
    "react-dom": "^19",
    "react-router": "7",
    "styled-components": "^6.1.19",
    "tailwindcss": "^4.0.6",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bun": "latest",
    "@types/ioredis": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "jsdom": "^27.0.0"
  }
