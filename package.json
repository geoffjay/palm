    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/themes": "^3.2.1",
    "bun-plugin-tailwind": "^0.0.14",
    "ioredis": "^5.7.0",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.3",
    "react": "^19",
    "react-dom": "^19",
    "react-router": "7",
    "tailwindcss": "^4.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/bun": "latest",
    "@types/ioredis": "^5.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
